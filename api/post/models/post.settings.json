{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titulo": {
      "type": "string"
    },
    "seo": {
      "type": "uid",
      "targetField": "titulo"
    },
    "publicacion": {
      "type": "date"
    },
    "users_permissions_role": {
      "plugin": "users-permissions",
      "model": "role"
    },
    "categorias": {
      "via": "posts",
      "collection": "categoria"
    },
    "contenido": {
      "type": "richtext"
    },
    "imagen": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    }
  }
}
